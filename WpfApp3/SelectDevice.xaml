<Window x:Class="CAN_Sharp.SelectDevice"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CAN_Sharp"
        mc:Ignorable="d"
        Title="SelectDevice" Height="300" Width="500">
    <Grid>
        <ListBox x:Name="lbSd" HorizontalAlignment="Left" Height="200" Margin="10,10,0,0" VerticalAlignment="Top" Width="472"/>
        <Button x:Name="buttonSelect" Content="Select" HorizontalAlignment="Left" Margin="225,226,0,0" VerticalAlignment="Top" Width="50" IsDefault="True" Click="buttonSelect_Click"/>
    </Grid>
</Window>
